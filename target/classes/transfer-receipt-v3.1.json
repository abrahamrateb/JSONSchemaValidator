{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TransferReceipt",
  "description": "Transfer Receipt SDEM",
  "properties": {
    "identifier": {
      "description": "Unique identifier for the transfer receipt, generated by the source system.",
      "example": "28686701",
      "type": "string",
      "pattern": "^.*$"
    },
    "transferShipmentIdentifier": {
      "description": "Unique Transfer Shipment Identifier for which the goods are being received on. Generated by the source system.",
      "example": "515835194",
      "type": "string",
      "pattern": "^.*$"
    },
    "transferBatchReference": {
      "description": "Unique reference for the dispatch of a delivery.",
      "example": "89479595",
      "type": "string",
      "pattern": "^.*$"
    },
    "transactionDateTime": {
      "description": "Date and time on which the the receipt happens or when the item was gunned / scanned at the receiving location(Store / DC).",
      "example": "2021-04-30T00:00:00+12",
      "type": "string",
      "format": "date-time"
    },
    "sendingLocationIdentifier": {
      "description": "Identifies where the goods are shipped from. This will be TWG location ID.",
      "example": "888",
      "type": "string",
      "pattern": "^.*$"
    },
    "receivingLocationIdentifier": {
      "description": "Identifies where the goods are shipped to. This is the location the goods are receipted. This will be TWG location ID.",
      "example": "111",
      "type": "string",
      "pattern": "^.*$"
    },
    "lines": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "item": {
            "type": "object",
            "properties": {
              "identifier": {
                "description": "Item master identifier as supplied from MDM.",
                "example": "182672",
                "type": "string",
                "pattern": "^.*$"
              },
              "barcode": {
                "description": "Primary Global Trade Item Number assigned to the Item Identifier within its respective source system.",
                "type": "object",
                "additionalProperties": false,
                "properties": {
                  "format": {
                    "description": "List of all the potential barcode format",
                    "enum": [
                      "ADD2",
                      "ADD5",
                      "ALL",
                      "ALL1D",
                      "ALL2D",
                      "AUSTRALIANPOST4STATE",
                      "AZTEC",
                      "BCDMATRIX",
                      "CODABAR",
                      "CODE128",
                      "CODE32",
                      "CODE39",
                      "CODE39EXTENDED",
                      "CODE93",
                      "CODE93EXTENDED",
                      "DATALOGIC2OF5",
                      "DATAMATRIX",
                      "EAN",
                      "EAN128",
                      "EAN13",
                      "EAN8",
                      "GS1DATABAR",
                      "IATA2OF5",
                      "INDUSTRY2OF5",
                      "INTELLIGENTMAIL",
                      "INTERLEAVED2OF5",
                      "INVERTED2OF5",
                      "MATRIX2OF5",
                      "MICROPDF417",
                      "PATCHCODE",
                      "PDF417",
                      "PLANET",
                      "PLU",
                      "POSTNET",
                      "QRCODE",
                      "ROYALPOST4STATE",
                      "UNKNOWN",
                      "UPC",
                      "UPCA",
                      "UPCE",
                      "UPU4STATE"
                    ]
                  },
                  "value": {
                    "description": "String without carriage return",
                    "type": "string",
                    "pattern": "^.*$"
                  },
                  "primary": {
                    "type": "boolean"
                  }
                }
              }
            }
          },
          "fromDisposition": {
            "description": "Identifies what bucket the item is being adjusted from.",
            "example": "Unavailable",
            "enum": [
              "Available To Sell",
              "Unavailable",
              "Customer Order Reserve",
              "Not Set",
              "Cross Dock"
            ]
          },
          "toDisposition": {
            "description": "Identifies what bucket the item is being adjusted into.",
            "example": "Available To Sell",
            "enum": [
              "Available To Sell",
              "Unavailable",
              "Customer Order Reserve",
              "Not Set",
              "Cross Dock"
            ]
          },
          "quantity": {
            "type": "integer",
            "description": "Number of units receipted.",
            "example": 10
          },
          "receiptType": {
            "description": "Type of receipt associated with this transaction.",
            "example": "Transshipment",
            "enum": [
              "Receipt",
              "Transshipment",
              "Adjustment"
            ]
          },
          "transferOrderIdentifier": {
            "description": "String without carriage return",
            "type": "string",
            "pattern": "^.*$",
            "example": "10932276"
          },
          "inventoryAdjustmentIndicator": {
            "type": "boolean",
            "description": "Defines whether the sending location needs a stock adjustment or not.",
            "example": false
          }
        }
      }
    }
  }
}