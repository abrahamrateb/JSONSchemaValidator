                      "QRCODE",
                      "ROYALPOST4STATE",
                      "UNKNOWN",
                      "UPC",
                      "UPCA",
                      "UPCE",
                      "UPU4STATE"
                    ]
                  },
                  "value": {
                    "description": "String without carriage return",
                    "type": "string",
                    "pattern": "^.*$"
                  },
                  "primary": {
                    "type": "boolean"
                  }
                }
              },
