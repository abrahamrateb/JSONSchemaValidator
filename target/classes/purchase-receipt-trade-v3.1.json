{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PurchaseReceiptTrade",
  "description": "Purchase Receipt Trade SDEM",
  "properties": {
    "identifier": {
      "description": "Unique identifier for this particular transaction, generated by the source system.",
      "example": "571011400361",
      "type": "string",
      "pattern": "^.*$"
    },
    "purchaseOrderIdentifier": {
      "description": "Unique identifier for the purchase order the receipt pertains to.",
      "example": "10932276",
      "type": "string",
      "pattern": "^.*$"
    },
    "advancedShippingNoticeIdentifier": {
      "description": "Unique identifier on the Advanced Shipping Notice given by the supplier for overseas purchase orders.",
      "example": "10932276/1",
      "type": "string",
      "pattern": "^.*$"
    },
    "transactionDateTime": {
      "description": "Date and time on which the the receipt happens or when the item was gunned / scanned at the receiving location(Store / DC).",
      "example": "2021-04-30T00:00:00+12",
      "type": "string",
      "format": "date-time"
    },
    "receivingLocationIdentifier": {
      "description": "Identifies where the goods are shipped to. This will be a TWG location ID.",
      "example": "177",
      "type": "string",
      "pattern": "^.*$"
    },
    "masterBillNumber": {
      "description": "Air or Ocean Bill Number issued by a carrier, it represents the contract of carriage for the goods.",
      "example": "AAV01209556",
      "type": "string",
      "pattern": "^.*$"
    },
    "receiptType": {
      "description": "Type of receipt associated with this transaction.",
      "example": "Receipt",
      "enum": [
        "Receipt",
        "Transshipment",
        "Adjustment"
      ]
    },
    "containerIdentifier": {
      "description": "This is a unique identifier of the shipping container that stored the goods for transportation.",
      "example": "FXLU2042824",
      "type": "string",
      "pattern": "^.*$"
    },
    "lines": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "item": {
            "type": "object",
            "properties": {
              "identifier": {
                "description": "Item master identifier as supplied from MDM.",
                "example": "428361",
                "type": "string",
                "pattern": "^.*$"
              },
              "barcode": {
                "description": "Primary Global Trade Item Number assigned to the Item Identifier within its respective source system.",
                "type": "object",
                "additionalProperties": false,
                "properties": {
                  "format": {
                    "description": "List of all the potential barcode format",
                    "enum": [
                      "ADD2",
                      "ADD5",
                      "ALL",
                      "ALL1D",
                      "ALL2D",
                      "AUSTRALIANPOST4STATE",
                      "AZTEC",
                      "BCDMATRIX",
                      "CODABAR",
                      "CODE128",
                      "CODE32",
                      "CODE39",
                      "CODE39EXTENDED",
                      "CODE93",
                      "CODE93EXTENDED",
                      "DATALOGIC2OF5",
                      "DATAMATRIX",
                      "EAN",
                      "EAN128",
                      "EAN13",
                      "EAN8",
                      "GS1DATABAR",
                      "IATA2OF5",
                      "INDUSTRY2OF5",
                      "INTELLIGENTMAIL",
                      "INTERLEAVED2OF5",
                      "INVERTED2OF5",
                      "MATRIX2OF5",
                      "MICROPDF417",
