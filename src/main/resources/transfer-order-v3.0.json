{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TransferOrder",
  "description": "Transfer Order SDEM",
  "properties": {
    "identifier": {
      "description": "Unique identifier for a transfer order",
      "example": "2457890",
      "type": "string",
      "pattern": "^.*$"
    },
    "deliveryDateTime": {
      "description": "Expected delivery date and time for the transfer to TWG store or warehouse",
      "type": "string",
      "format": "date-time",
      "example": "2018-11-13T20:20:39+00:00"
    },
    "status": {
      "description": "Status of the transfer order.",
      "enum": [
        "Approved",
        "Completed",
        "Cancelled",
        "Suggested",
        "Pending"
      ]
    },
    "sendingLocationIdentifier": {
      "description": "Identifies where the goods are shipped from. This will be a TWG location ID.",
      "example": "888",
      "type": "string",
      "pattern": "^.*$"
    },
    "receivingLocationIdentifier": {
      "description": "Identifies where the goods are shipped to. This will be a TWG location ID.",
      "example": "177",
      "type": "string",
      "pattern": "^.*$"
    },
    "reserveDisposition": {
      "description": "Identifies the inventory bucket that the items on the transfer order will be reserved in.",
      "enum": [
        "Available To Sell",
        "Unavailable",
        "Customer Order Reserve",
        "Not Set",
        "Cross Dock"
      ]
    },
    "lines": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "item": {
            "type": "object",
            "properties": {
              "identifier": {
                "description": "Item master identifier as supplied from MDM.",
                "example": "428361",
                "type": "string",
                "pattern": "^.*$"
              },
              "barcode": {
                "description": "Primary Global Trade Item Number assigned to the Item Identifier within its respective source system.",
                "type": "object",
                "additionalProperties": false,
                "properties": {
                  "format": {
                    "description": "List of all the potential barcode format",
                    "enum": [
                      "ADD2",
                      "ADD5",
                      "ALL",
                      "ALL1D",
                      "ALL2D",
                      "AUSTRALIANPOST4STATE",
                      "AZTEC",
                      "BCDMATRIX",
