{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OrderReleaseDropship",
  "description": "Order Release Dropship SDEM",
  "required": [
    "orderIdentifier"
  ],
  "additionalProperties": true,
  "properties": {
    "orderIdentifier": {
      "description": "Order identifier sent from the source system",
      "example": "19370256",
      "type": "string",
      "pattern": "^.*$"
    },
    "releaseIdentifier": {
      "description": "Unique order release identifier from the producer system",
      "example": "000343138",
      "type": "string",
      "pattern": "^.*$"
    },
    "senderCompanyCode": {
      "description": "An internal identifier set by The Warehouse Group to represent a specific business/ company entity. Identifies the sender company code to which the order release details data belongs to.",
      "example": "TWL",
      "enum": [
        "NLG",
        "T7",
        "TWL",
        "WSL",
        "1DAY",
        "TM"
      ]
    },
    "supplierIdentifier": {
      "description": "Identifies the Supplier Site assigned to the item. Supplier site identifier will be supplied along with the release.",
      "example": "B467",
      "type": "string",
      "pattern": "^.*$"
    },
    "supplierName": {
      "description": "Identifies the name of the supplier that fulfills the order on behalf of The Warehouse Group. Supplier name will be supplied along with the release.",
      "example": "TGV Limited",
      "type": "string",
      "pattern": "^.*$"
    },
    "orderDatetime": {
      "description": "Date and time when the order was captured by the source system.",
      "example": "2021-09-28T02:20:11:342+12:00.",
      "type": "string",
      "format": "date-time"
    },
    "fulfilmentProcessCode": {
      "description": "The fulfilment code to be applied for processing the order based on the order source and brand from which the order was placed.",
      "example": "TW",
      "type": "string",
      "pattern": "^.*$"
    },
